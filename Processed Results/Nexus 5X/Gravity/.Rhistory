library(readxl)
Gravity_Raw <- read_excel("Gravity - Raw.xlsx")
View(Gravity_Raw)
library(tidyverse)
library(plotly)
library(IRdisplay)
data <- Gravity_Raw
# Violin plot with ggplot2
p <- data %>% ggplot(aes(x="", y = Standard))  +
geom_violin() +
geom_boxplot(width=0.1) +
theme(axis.title.x = element_blank())
p + ggtitle("Gravity sensor") +
ylab("Energy in Joule")
