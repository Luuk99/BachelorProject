library(readxl)
Gyroscope_Raw <- read_excel("Gyroscope - Raw.xlsx")
View(Gyroscope_Raw)
library(tidyverse)
library(plotly)
library(IRdisplay)
data <- Gyroscope_Raw
# Violin plot with ggplot2
p <- data %>% ggplot(aes(x="", y = Standard))  +
geom_violin() +
geom_boxplot(width=0.1) +
theme(axis.title.x = element_blank())
p + ggtitle("Gyroscope") +
ylab("Energy in Joule")
