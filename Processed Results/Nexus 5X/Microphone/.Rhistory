library(readxl)
Microphone_Raw <- read_excel("Microphone - Raw.xlsx")
View(Microphone_Raw)
library(tidyverse)
library(plotly)
library(IRdisplay)
data <- Microphone_Raw
# Violin plot with ggplot2
p <- data %>% ggplot(aes(x="", y = Standard))  +
geom_violin() +
geom_boxplot(width=0.1) +
theme(axis.title.x = element_blank())
p + ggtitle("Microphone") +
ylab("Energy in Joule")
